<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <include>
      <uri>model://sun</uri>
    </include>

    <include>
      <uri>model://ground_plane</uri>
    </include>

    <model name="model">
      <link name="link">
        <pose>0 0.5 1.5 0 0 0</pose>
        <collision name="collision">
          <geometry>
            <box>
              <size>1 1 1</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>1 1 1</size>
            </box>
          </geometry>
        </visual>
      </link>
      <joint name="joint" type="revolute">
        <pose>0 -0.5 -0.5 0 0 0</pose>
        <parent>world</parent>
        <child>link</child>
        <axis>
          <xyz>1 0 0</xyz>
          <limit>
            <lower>-0.785398</lower>
            <upper> 0.785398</upper>
          </limit>
        </axis>
      </joint>
      <plugin name="moveCoG" filename="libMoveCoGPlugin.so"/>
    </model>
  </world>
</sdf>
